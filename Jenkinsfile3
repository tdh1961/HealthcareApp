pipeline {
    agent any
    tools{
        maven 'M2_HOME'
    }
    stages{
        stage ('sonarqube scan'){
            steps{
                withSonarQubeEnv('sonarQube'){
                sh 'mvn verify org.sonarsource.scanner.maven:sonar-maven-plugin:sonar -Dsonar.projectKey=tdh1961_week16-healthCarePortail'
                }       
                }
        }
        stage ('all maven commands'){
            steps{
                sh 'mvn clean test compile install package'
            }
        }
       
    }
}